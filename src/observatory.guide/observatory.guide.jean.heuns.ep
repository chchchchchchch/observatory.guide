//gallery.constantvzw.org/index.php/Techno-Galactic-Software-Observatory/PWFU3350\n% http://gallery.constantvzw.org/index.php/Techno-Galactic-Software-Observatory/PWFU3361\n% SHOW: tgsoimages.svg kuleuven1\n\n\n% RELATESTO:\n% SOURCE:\n\n","attribs":"+2*3*4+6|h+iw*0|1+1*0+2+1*5+k+v*5+n|1+2*0|1+1|1+1*0|4+5w+2*0+2e|1+1+2*0|1+2f*0+w|1+1*0|2+2+2*0+9|3+d"},"pool":{"numToAttrib":{"0":["author","a.PRabeGfk5jCXUq8V"],"1":["author",""],"2":["removed","true"],"3":["bold","true"],"4":["underline","true"],"5":["author","a.I4FbrrCTDQNN8GIf"]},"nextNum":6},"head":121,"chatHead":-1,"publicStatus":false,"passwordHash":null,"savedRevisions":[]},"globalAuthor:a.PRabeGfk5jCXUq8V":{"colorId":"#0c8ae5","name":"CH","timestamp":1517858520058,"padIDs":"observatory.guide.jean.heuns"},"globalAuthor:":null,"globalAuthor:a.I4FbrrCTDQNN8GIf":{"colorId":"#8af9d7","name":"FS","timestamp":1517851290970,"padIDs":"observatory.guide.jean.heuns"}}