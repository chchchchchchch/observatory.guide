ervatory.guide.bestiary:revs:2237":{"changeset":"Z:6em>5|1u=3r7=m*6+5$ a fe","meta":{"author":"a.CR709lDV4oQ6pPzO","timestamp":1522221036294}},"pad:observatory.guide.bestiary:revs:2238":{"changeset":"Z:6er>4|1u=3r7=r*6+4$dora","meta":{"author":"a.CR709lDV4oQ6pPzO","timestamp":1522221036797}},"pad:observatory.guide.bestiary:revs:2239":{"changeset":"Z:6ev<1|1s=3nt=h-1$","meta":{"author":"a.CR709lDV4oQ6pPzO","timestamp":1522221053906}},"pad:observatory.guide.bestiary:revs:224":{"changeset":"Z:39h>1|18=2c7*6|1+1$\n","meta":{"author":"a.CR709lDV4oQ6pPzO","timestamp":1522179687879}},"pad:observatory.guide.bestiary:revs:2240":{"changeset":"Z:6eu<1|1s=3nt=g-1$","meta":{"author":"a.CR709lDV4oQ6pPzO","timestamp":1522221054187}},"pad:observatory.guide.bestiary:revs:2241":{"changeset":"Z:6et>2|1s=3nt=g*6+2$g ","meta":{"author":"a.CR709lDV4oQ6pPzO","timestamp":1522221054687}},"pad:observatory.guide.bestiary:revs:2242":{"changeset":"Z:6ev>67|2m=5a4|1-2r-2w*6+b*6*2+h*6|1+30*6+h*6*2+1f*6+50*6*2+p*6+h$Dowse is a transparent proxy facilitating the awareness of ingoing and outgoing connections, from, to, and within a local area network.\nDowse provides a central point of soft control for all local traffic: from ARP traffic (layer 2) to TCP/IP (layers 3 and 4) as well as application space, by chaining a firewall setup to a trasparent proxy setup. A core feature for Dowse is that of hiding all the complexity of such a setup.","meta":{"author":"a.CR709lDV4oQ6pPzO","timestamp":1522221241197}},"pad:observatory.guide.bestiary:revs:2243":{"changeset":"Z:6l2<1|2m=5a4=3r|1-1$","meta":{"author":"a.CR709lDV4oQ6pPzO","timestamp":1522221247893}},"pad:observatory.guide.bestiary:revs:2244":{"changeset":"Z:6l1>1|2m=5a4=3r*6+1$ ","meta":{"author":"a.CR709lDV4oQ6pPzO","timestamp":1522221248494}},"pad:observatory.guide.bestiary:revs:2245":{"changeset":"Z:6l2>1|1k=3bi=8*6+1$H","meta":{"author":"a.CR709lDV4oQ6pPzO","timestamp":1522221306997}},"pad:observatory.guide.bestiary:revs:2246":{"changeset":"Z:6l3>